{
    "appType": "kontext",
    "logRedis": {
        "address": "localhost",
        "database": "1",
        "key": "kontext_log"
    },
    "logFiles": {
        "worklogPath": "/var/log/klogproc/worklog/worklog.txt",
        "srcPath": "/var/log/kontext-logs",
        "partiallyMatchingFiles": false
    },
    "elasticSearch": {
        "server": "http://192.168.1.23:9200",
        "index": "cnk-logs",
	    "pushChunkSize": 20,
        "scrollTtl": "3m"
    },
    "influxDb": {
        "server": "http://192.168.1.24:8086",
        "pushChunkSize": 1,
        "database": "klogproc",
        "measurement": "procTime",
        "retentionPolicy": "midterm"
    },
	"geoIpDbPath": "/usr/local/geodata/GeoLite2-City.mmdb",
	"localTimezone": "+01:00",
	"anonymousUsers": [1531, 1532],
    "logPath": "/var/log/klogproc/klogproc.log",
    "recordUpdate": {
        "searchChunkSize": 10,
        "filters": [
            {
                "disabled": "false",
                "fromDate": "2017-01-01T10:00:00",
                "toDate": "2017-02-07T10:00:00",
                "ipAddress": "141.20.212.106",
                "userAgent": "Mozilla"

            }
        ],
        "update": {
            "notice": "Power outage"
        },
        "removeKey": "foo"
    }
}